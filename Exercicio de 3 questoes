// CALCULAR A SOMA DOS NUMEROS IMPARES MAOIRES QUE 10 E MENORES QUE 30

var soma = 0 
    
    for (let i = 11; i < 30; i++){
        if (i%2 == 1){
            soma +=i
        }
    }
    console.log (soma)
    document.write(soma)
  
// IMPRIMIR OS NUMEROS IMPARES MENORES QUE 100

    for (let i = 1; i<100; i++){
        if (i%2 !=0){
            console.log(i)
            document.write(i)
            document.write('<br>')
        }
    }


